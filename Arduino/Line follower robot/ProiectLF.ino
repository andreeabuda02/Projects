/////////////////////////////////////////////////////////////////////////////////////numai pentru senzorul ultrasonic

void loop() {

  // Clears the trigPin
  digitalWrite(trigPin, LOW);
  delayMicroseconds(2);
  // Sets the trigPin on HIGH state for 10 micro seconds
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  // Reads the echoPin, returns the sound wave travel time in microseconds
  durata = pulseIn(echoPin, HIGH);
  // Calculating the distance
  distanta = durata * 0.034 / 2;

  display.clearDisplay();

  if (distanta <= 15) {
    StartMotor(mpin00, mpin01, 1, 0);
    StartMotor(mpin10, mpin11, 0, 0);
    display.drawBitmap(0,0,directii[1],OLED_WIGTH,OLED_HEIGHT,WHITE);
  } else {
    StartMotor(mpin00, mpin01, 0, 200);
    StartMotor(mpin10, mpin11, 1, 200);
    display.drawBitmap(0,0,directii[0],OLED_WIGTH,OLED_HEIGHT,WHITE);
    delay(200);  // Cât timp e motorul pornit
  }

  display.display();

  //delay (1000); // Cât timp e motorul pornit
}